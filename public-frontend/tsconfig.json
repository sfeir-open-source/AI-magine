{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "es2016",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "types": [
      "vitest/globals",
      "@testing-library/jest-dom",
      "vite/client"
    ]
  }
}
